{
  "title": "Angularrera sarrera",
  "og_url": "https://irontec.github.io/slides-angular/",
  "og_image": "https://irontec.github.io/slides-angular/media/images/logo.png",
  "options": {
    "controls": true,
    "progress": true,
    "history": true,
    "center": true,
    "slideNumber": true,
    "touch": true,
    "hideAddressBar": true,
    "mouseWheel": true
  },
  "plugins": {
    "search": true,
    "zoom":   true,
    "menu":   true,
    "notes":  true,
    "highlightjs": true,
    "ga": "UA-130231733-5"
  },
  "serviceWorker": true,
  "slides": [
    {"path": "index.md", "attrs": {"class":"intro"}},
    [
      "01-intro/*"
    ],
    [
      "02-start/*"
    ],
    [
      "03-modules/*"
    ],
    [
      "04-components/*"
    ],
    [
      "05-directives/*"
    ],
    [
      "06-pipes/*"

    ],
    [
      "07-services/*"
    ],
    [
      "08-http/*"
    ],
    [
      "09-routing/*"

    ],
    {"path": "end.md", "attrs": {"class":"last bg-red"}}
  ]
}
